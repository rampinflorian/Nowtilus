{% extends 'base.html.twig' %}

{% block title %}FluxRSS{% endblock %}

{% block body %}
    <h1>FluxRSS</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ fluxrss.id }}</td>
        </tr>
        <tr>
            <th>Title</th>
            <td>{{ fluxrss.title }}</td>
        </tr>
        <tr>
            <th>Link</th>
            <td>{{ fluxrss.link }}</td>
        </tr>
        <tr>
            <th>IsActive</th>
            <td>{{ fluxrss.isActive ? 'Yes' : 'No' }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('fluxrss_index') }}">back to list</a>

    <a href="{{ path('fluxrss_edit', {'id': fluxrss.id}) }}">edit</a>

    {{ include('flux_rss/_delete_form.html.twig') }}
{% endblock %}
