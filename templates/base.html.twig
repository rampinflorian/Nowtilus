<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <title>{% block title %}{% endblock %} - Nowtilus</title>
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport"/>
    <meta content="" name="description"/>
    <meta content="" name="author"/>

    <!-- ================== BEGIN BASE CSS STYLE ================== -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet"/>
    <link rel="icon" href="{{ asset('assets/img/nowtilus.ico') }}"/>

    {% block head_css %}
        <link href="{{ asset('assets/css/default/app.min.css') }}" rel="stylesheet"/>
        <!-- BLUE -->
        <link href="{{ asset('assets/css/default/theme/orange.min.css') }}" rel="stylesheet"/>
    {% endblock %}

    {% block stylesheets %}{% endblock %}

</head>
<body>
<!-- begin #page-loader -->
<div id="page-loader" class="fade show"><span class="spinner"></span></div>
<!-- end #page-loader -->

<!-- begin #page-container -->
<div id="page-container" class="fade page-without-sidebar page-header-fixed">
    <!-- begin #header -->
    <div id="header" class="header navbar-inverse">
        <!-- begin navbar-header -->
        <div class="navbar-header">
            <a href="{{ path('dashboard') }}" class="navbar-brand"><img src="{{ asset('assets/img/nowtilus.ico') }}"> <b>&nbsp;&nbsp;Now</b>Tilus</a>
            <button type="button" class="navbar-toggle" data-click="sidebar-toggled">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
        </div>
        <!-- end navbar-header -->
        <!-- begin navbar-collapse -->
        <div class="collapse navbar-collapse" id="top-navbar">
            <ul class="nav navbar-nav">
                <li>
                    <a href="{{ path('dashboard') }}">
                        <i class="fa fa-gem fa-fw mr-1 mr-md-0"></i> Tableau de bord
                    </a>
                </li>
                <li class="dropdown dropdown-lg">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-th-large fa-fw mr-1 mr-md-0"></i> Menu <b class="caret ml-1 ml-md-0"></b></a>
                    <div class="dropdown-menu dropdown-menu-lg">
                        <div class="row">
                            <div class="col-lg-4 col-md-4">
                                <div class="f-w-700 text-inverse mb-2">Florian</div>
                                <div class="row m-b-20">
                                    <div class="col-lg-6">
                                        <ul class="nav d-block">
                                            <li><a class="text-ellipsis text-muted" href="{{ path('water_quality_index') }}"><i class="fa fa-eye-dropper"></i><span> Aquarium - Qualité de l'eau</span></a></li>
                                            <li><a class="text-ellipsis text-muted" href="{{ path('raidwow_index') }}"><i class="fa fa-eye-dropper"></i><span> WoW - Fiche personnage</span></a></li>
                                        </ul>
                                    </div>
                                    <div class="col-lg-6">
                                        <ul class="nav d-block">
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-4">
                                <div class="f-w-700 text-inverse mb-2">Marina</div>
                                <div class="row m-b-20">
                                    <div class="col-lg-6">
                                        <ul class="nav d-block">
                                            <li class="text-ellipsis text-muted"><i class="fa fa-users"></i><span> Travail - Gestion du planning</span></li>
                                        </ul>
                                        <ul class="sub-menu">
                                            <li><a class="text-ellipsis text-muted" href="{{ path('gestion_client') }}">Mon planning</a></li>
                                            <li><a class="text-ellipsis text-muted" href="{{ path('gestion_client_creationRDV') }}">Nouveau rendez-vous</a></li>
                                            <li><a class="text-ellipsis text-muted" href="{{ path('gestion_client_creationClient') }}">Nouveau client</a></li>
                                        </ul>
                                    </div>
                                    <div class="col-lg-6">
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-4">
                                <div class="f-w-700 text-inverse mb-2">Commun</div>
                                <div class="row m-b-20">
                                    <div class="col-lg-6">
                                        <ul class="nav d-block">
                                            <li class="text-ellipsis text-muted"><i class="fa fa-edit"></i><span> Paramétrages</span></li>
                                        </ul>
                                        <ul class="sub-menu">
                                            <li><a class="text-ellipsis text-muted" href="{{ path('fluxrss_index') }}">Flux RSS</a></li>
                                        </ul>
                                    </div>
                                    <div class="col-lg-6">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </li>
            </ul>
        </div>
        <!-- end navbar-collapse -->
        <!-- begin header-nav -->
        <ul class="navbar-nav navbar-right">

            <li class="dropdown navbar-user">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                    <span class="d-none d-md-inline">{{ app.user.name }} {{ app.user.surname }}</span> <b class="caret"></b>
                </a>
                <div class="dropdown-menu dropdown-menu-right">

                    <a href="{{ path('app_logout') }}" class="dropdown-item">Déconnexion</a>
                </div>
            </li>
        </ul>
        <!-- end header-nav -->
    </div>
    <!-- end #header -->

    {% block body %}{% endblock %}


    <!-- begin scroll to top btn -->
    <a href="javascript:;" class="btn btn-icon btn-circle btn-success btn-scroll-to-top fade" data-click="scroll-top"><i class="fa fa-angle-up"></i></a>
    <!-- end scroll to top btn -->
</div>
<!-- end page container -->

<!-- ================== BEGIN BASE JS ================== -->
{% block head_js %}
    <script src="{{ asset('assets/js/app.min.js') }}"></script>
    <script src="{{ asset('assets/js/theme/default.min.js') }}"></script>
{% endblock %}

<!-- ================== END BASE JS ================== -->

{% block javascripts %}{% endblock %}
</body>
</html>